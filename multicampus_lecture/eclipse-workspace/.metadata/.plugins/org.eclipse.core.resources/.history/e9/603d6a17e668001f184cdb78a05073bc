package polymorphism3;

public class TVUser {

	public static void main(String[] args) {
		// 컨테이너를 생성한다. 
		TVContainer container = new TVContainer();
		
		// 컨테이너로부터 사용할 객체를 획득한다. 		
		TV tv = (TV) container.getBean(args[0]);
		tv.powerOn();
		tv.volumeUp();
		tv.volumeDown();
		tv.powerOff();
	}

}
